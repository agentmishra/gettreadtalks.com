{
  "name": "treadtalks-gatsby",
  "description": "TREAD Talks website built with Gatsby",
  "version": "2.0.0",
  "author": "Luke McDonald <thelukemcdonald@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukemcdonald/gettreadtalks.com"
  },
  "scripts": {
    "build": "NODE_ENV=production cross-env NODE_OPTIONS='-r esm' gatsby build",
    "develop": "cross-env NODE_OPTIONS='-r esm' gatsby develop",
    "start": "npm run develop",
    "serve": "cross-env NODE_OPTIONS='-r esm' gatsby serve",
    "clean": "gatsby clean"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "react/jsx-props-no-spreading": "off",
      "react/prop-types": 0,
      "no-shadow": "off",
      "no-void": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@raae/gatsby-remark-oembed": "^0.1.1",
    "@sindresorhus/slugify": "^1.1.0",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.3.3",
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "firebase": "^8.10.0",
    "formik": "^2.2.9",
    "gatsby": "^3.12.1",
    "gatsby-plugin-feed": "^3.12.0",
    "gatsby-plugin-firebase": "0.2.0-beta.4",
    "gatsby-plugin-image": "^1.12.0",
    "gatsby-plugin-manifest": "^3.12.0",
    "gatsby-plugin-netlify": "^3.12.0",
    "gatsby-plugin-postcss": "^4.12.0",
    "gatsby-plugin-react-helmet": "^4.12.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "gatsby-plugin-robots-txt": "^1.6.9",
    "gatsby-plugin-sharp": "^3.12.0",
    "gatsby-plugin-sitemap": "^4.8.0",
    "gatsby-remark-responsive-iframe": "^4.9.0",
    "gatsby-source-airtable": "^2.2.1",
    "gatsby-source-filesystem": "^3.12.0",
    "gatsby-transformer-remark": "^4.9.0",
    "gatsby-transformer-sharp": "^3.12.0",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^6.1.0",
    "striptags": "^3.2.0",
    "tailwindcss": "^2.2.9",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "tailwindcss-responsive-embed": "^1.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-wesbos": "lukemcdonald/eslint-config-wesbos",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "esm": "^3.2.25",
    "prettier": "^2.3.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
